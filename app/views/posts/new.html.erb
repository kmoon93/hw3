<%= form_with :model => @post do |form| %>
  <p>
    <%= form.label "title", "Title Post" %>
    <%= form.text_field "title" %>
  </p>

<p>
    <%= form.label "long description", "Description" %>
    <%= form.text_field "long description" %>
  </p>

<p>
    <%= form.label "date", "Posted Date" %>
    <%= form.text_field "date" %>
  </p>

<% end %>